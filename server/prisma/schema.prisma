datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  nome  String
}

model Admin {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model mail_schedule {
  id      Int    @id @default(autoincrement())
  content String
  img     String
}
